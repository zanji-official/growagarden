if identifyexecutor() == "Delta" then
    -- Clear existing CoreGui UI
    for _, v in pairs(game:GetService("CoreGui"):GetChildren()) do
        pcall(function()
            v:Destroy()
        end)
    end

    -- Create warning GUI
    local gui = Instance.new("ScreenGui")
    gui.Name = "SecurityWarning"
    gui.IgnoreGuiInset = true
    gui.ResetOnSpawn = false
    if syn and syn.protect_gui then
        syn.protect_gui(gui)
    end
    gui.Parent = game:GetService("CoreGui")

    -- Create main container
    local container = Instance.new("Frame")
    container.AnchorPoint = Vector2.new(0.5, 0.5)
    container.Position = UDim2.new(0.5, 0, 0.5, 0)
    container.Size = UDim2.new(0.6, 0, 0.55, 0)
    container.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    container.BorderSizePixel = 0
    container.BackgroundTransparency = 0.1
    container.Parent = gui

    local uicorner = Instance.new("UICorner", container)
    uicorner.CornerRadius = UDim.new(0, 8)

    -- Add vertical layout
    local layout = Instance.new("UIListLayout", container)
    layout.FillDirection = Enum.FillDirection.Vertical
    layout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    layout.VerticalAlignment = Enum.VerticalAlignment.Top
    layout.Padding = UDim.new(0, 10)

    -- Helper: Create text label
    local function createLabel(text, color)
        local label = Instance.new("TextLabel")
        label.BackgroundTransparency = 1
        label.Size = UDim2.new(1, -20, 0, 40)
        label.TextColor3 = color or Color3.new(1, 1, 1)
        label.Text = text
        label.Font = Enum.Font.SourceSansBold
        label.TextScaled = true
        label.TextWrapped = true
        label.Parent = container
        return label
    end

    -- Warning messages
    local danger = "⚠️"
    createLabel(danger .. " Unsafe Executor Detected: Delta " .. danger, Color3.fromRGB(255, 255, 0))
    createLabel("Warning: Delta has been flagged as unsafe software.", Color3.fromRGB(255, 85, 85))
    createLabel("This executor may log sensitive data and is highly detectable.", Color3.fromRGB(255, 255, 255))
    createLabel("For your safety, please switch to a trusted executor below:", Color3.fromRGB(200, 200, 200))

    -- Executor options
    local executors = {
        {name = "KRNL", link = "https://krnl.cat/"},
        {name = "Codex", link = "https://codex.lol/"},
        {name = "Arceus X", link = "https://spdmteam.com/index"},
        {name = "Fluxus", link = "https://fluxus.team/download/"}
    }

    local buttonsFrame = Instance.new("Frame", container)
    buttonsFrame.Size = UDim2.new(1, -20, 0, 120)
    buttonsFrame.BackgroundTransparency = 1

    local grid = Instance.new("UIGridLayout", buttonsFrame)
    grid.CellSize = UDim2.new(0.48, 0, 0, 40)
    grid.CellPadding = UDim2.new(0.04, 0, 0, 10)
    grid.FillDirectionMaxCells = 2
    grid.HorizontalAlignment = Enum.HorizontalAlignment.Center

    -- Create copy buttons
    for _, exec in ipairs(executors) do
        local btn = Instance.new("TextButton")
        btn.Text = "Copy " .. exec.name .. " Link"
        btn.TextScaled = true
        btn.Font = Enum.Font.SourceSansBold
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        btn.Size = UDim2.new(0, 200, 0, 40)
        btn.AutoButtonColor = true
        btn.BorderSizePixel = 0
        btn.Parent = buttonsFrame

        local corner = Instance.new("UICorner", btn)
        corner.CornerRadius = UDim.new(0, 6)

        btn.MouseButton1Click:Connect(function()
            if setclipboard then
                setclipboard(exec.link)
            end
            btn.Text = "Copied!"
            task.delay(1.5, function()
                btn.Text = "Copy " .. exec.name .. " Link"
            end)
        end)
    end

    -- Countdown
    local countdownLabel = Instance.new("TextLabel")
    countdownLabel.Size = UDim2.new(1, -20, 0, 35)
    countdownLabel.BackgroundTransparency = 1
    countdownLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    countdownLabel.Font = Enum.Font.SourceSansBold
    countdownLabel.TextScaled = true
    countdownLabel.TextWrapped = true
    countdownLabel.Parent = container

    -- Start countdown
    local seconds = 30
    spawn(function()
        while seconds > 0 do
            countdownLabel.Text = "Game will automatically close in " .. seconds .. " second" .. (seconds == 1 and "" or "s") .. "."
            wait(1)
            seconds -= 1
        end
        game:Shutdown()
    end)

    wait(math.huge)
    return
end
