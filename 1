if identifyexecutor() == 'Delta' then
    -- Cleanup existing UI
    for _, v in pairs(game:GetService("CoreGui"):GetChildren()) do
        pcall(function()
            if v.Name == "WarningGUI" then
                v:Destroy()
            end
        end)
    end

    -- Setup ScreenGui
    local gui = Instance.new("ScreenGui")
    gui.Name = "WarningGUI"
    gui.IgnoreGuiInset = true
    gui.ResetOnSpawn = false
    if syn and syn.protect_gui then
        syn.protect_gui(gui)
    end
    gui.Parent = game:GetService("CoreGui")

    -- Main Container
    local container = Instance.new("Frame")
    container.AnchorPoint = Vector2.new(0.5, 0.5)
    container.Position = UDim2.new(0.5, 0, 0.5, 0)
    container.Size = UDim2.new(0, 500, 0, 400)
    container.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    container.BorderSizePixel = 0
    container.BackgroundTransparency = 0
    container.Parent = gui

    local corner = Instance.new("UICorner", container)
    corner.CornerRadius = UDim.new(0, 12)

    local stroke = Instance.new("UIStroke", container)
    stroke.Thickness = 1
    stroke.Color = Color3.fromRGB(100, 100, 100)

    -- Layout Manager
    local layout = Instance.new("UIListLayout", container)
    layout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    layout.VerticalAlignment = Enum.VerticalAlignment.Top
    layout.Padding = UDim.new(0, 10)

    -- Executor Links
    local executors = {
        {name = "KRNL", link = "https://krnl.cat/"},
        {name = "Codex", link = "https://codex.lol/"},
        {name = "Arceus X", link = "https://spdmteam.com/index"},
        {name = "Fluxus", link = "https://fluxus.team/download/"}
    }

    local function createLabel(text, color)
        local label = Instance.new("TextLabel")
        label.Text = text
        label.Size = UDim2.new(1, -20, 0, 30)
        label.BackgroundTransparency = 1
        label.TextColor3 = color or Color3.new(1, 1, 1)
        label.Font = Enum.Font.GothamBold
        label.TextScaled = true
        label.TextWrapped = true
        label.Parent = container
        return label
    end

    local function createButton(exec)
        local btn = Instance.new("TextButton")
        btn.Text = "Copy " .. exec.name .. " Download Link"
        btn.Size = UDim2.new(0.9, 0, 0, 40)
        btn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        btn.TextColor3 = Color3.new(1, 1, 1)
        btn.Font = Enum.Font.Gotham
        btn.TextScaled = true
        btn.AutoButtonColor = true
        btn.BorderSizePixel = 0
        btn.Parent = container

        local btnCorner = Instance.new("UICorner", btn)
        btnCorner.CornerRadius = UDim.new(0, 6)

        btn.MouseButton1Click:Connect(function()
            if setclipboard then
                setclipboard(exec.link)
                btn.Text = "Link Copied!"
                task.delay(1.5, function()
                    btn.Text = "Copy " .. exec.name .. " Download Link"
                end)
            end
        end)
    end

    -- UI Content
    local warningIcon = "⚠️"
    createLabel(warningIcon .. " Delta Executor Detected " .. warningIcon, Color3.fromRGB(255, 220, 0))
    createLabel("Delta is flagged as a malicious executor.", Color3.fromRGB(255, 0, 0))
    createLabel("It collects your data and is highly detected.", Color3.fromRGB(255, 255, 255))
    createLabel("We strongly recommend switching to a trusted alternative:", Color3.fromRGB(255, 255, 255))

    for _, exec in ipairs(executors) do
        createButton(exec)
    end

    createLabel("These alternatives are stable, safe, and widely supported.", Color3.fromRGB(0, 255, 255))

    -- Countdown Timer
    local countdownLabel = Instance.new("TextLabel")
    countdownLabel.Size = UDim2.new(1, -20, 0, 30)
    countdownLabel.BackgroundTransparency = 1
    countdownLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    countdownLabel.Font = Enum.Font.Gotham
    countdownLabel.TextScaled = true
    countdownLabel.TextWrapped = true
    countdownLabel.Parent = container

    local seconds = 30
    task.spawn(function()
        while seconds > 0 do
            countdownLabel.Text = "This session will close in " .. seconds .. " second" .. (seconds == 1 and "" or "s") .. ". Please use a different executor."
            wait(1)
            seconds -= 1
        end
        game:Shutdown()
    end)

    return
end
